{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/pokemon.html",
      "allowedRoles": ["pokemon"]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/static/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/401.html"
    },
    "403": {
      "rewrite": "/403.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  }
}
